  "repository": {
    "type": "git",
    "url": "https://github.com/hastyy/vulpix"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^20.4.8",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  }
